{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src/**/*.vue", "src/**/*.ts", "src/**/*.d.ts"],
  "exclude": ["src/**/__tests__/*", "node_modules"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "esModuleInterop":true,
    "allowSyntheticDefaultImports":true,

  }
}
